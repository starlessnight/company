<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="Header">
	    <item name="android:textSize">@dimen/smaller_font</item>
	    <item name="android:textColor">@android:color/white</item>
	    <item name="android:textStyle">bold</item>
	    <item name="android:padding">15dp</item>
	    <item name="android:background">@color/header_background</item>
	</style>
	<style name="OverlayHeader">
	    <item name="android:textSize">@dimen/smaller_font</item>
	    <item name="android:textColor">@android:color/white</item>
	    <item name="android:textStyle">bold</item>
	    <item name="android:paddingTop">10dp</item>
	    <item name="android:paddingRight">15dp</item>
	    <item name="android:paddingBottom">8dp</item>
	    <item name="android:paddingLeft">15dp</item>
	    <item name="android:background">@color/pop_up_header_background</item>
	</style>
	<style name="PopUpHeader" parent="OverlayHeader"></style>
	<style name="OverlayBody">
	    <item name="android:textSize">@dimen/smaller_font</item>
	    <item name="android:textColor">@android:color/white</item>
	    <item name="android:paddingTop">22dp</item>
	    <item name="android:paddingRight">15dp</item>
	    <item name="android:paddingBottom">24dp</item>
	    <item name="android:paddingLeft">15dp</item>
	    <item name="android:background">@color/dark_gray</item>
	</style>
	<style name="SmallerText">
	    <item name="android:textSize">@dimen/smaller_font</item>
	    <item name="android:textColor">@android:color/white</item>
	</style>
	<style name="SmallerBoldText" parent="SmallerText">
	    <item name="android:textStyle">bold</item>
	</style>
	<style name="SmallestText">
	    <item name="android:textSize">@dimen/smallest_font</item>
	    <item name="android:textColor">@color/secondary_font</item>
	</style>
	<style name="MicroText">
	    <item name="android:textSize">@dimen/micro_font</item>
	    <item name="android:textColor">@color/secondary_font</item>
	    <item name="android:includeFontPadding">false</item>
	</style>
	<style name="MediumText">
	    <item name="android:textSize">@dimen/medium_font</item>
	    <item name="android:includeFontPadding">false</item>
	    <item name="android:textColor">@android:color/white</item>
	    <item name="android:paddingTop">5dp</item>
	</style>
	<style name="LargeBoldText">
	    <item name="android:textSize">@dimen/large_font</item>
	    <item name="android:textColor">@android:color/white</item>
	    <item name="android:textStyle">bold</item>
	</style>
	<style name="LargeText">
	    <item name="android:textSize">@dimen/large_font</item>
	    <item name="android:includeFontPadding">false</item>
	    <item name="android:textColor">@android:color/white</item>
	    <item name="android:paddingTop">5dp</item>
	</style>
	<style name="NormalTextField">
	    <item name="android:textSize">@dimen/smaller_font</item>
	    <item name="android:textColor">@android:color/white</item>
	    <item name="android:textColorHint">@color/secondary_font</item>
	    <item name="android:paddingTop">17dp</item>
		<item name="android:paddingRight">15dp</item>
		<item name="android:paddingBottom">17dp</item>
		<item name="android:paddingLeft">15dp</item>
		<item name="android:includeFontPadding">false</item>
		<item name="android:background">@drawable/normal_text_field</item>
	</style>
	<style name="SearchTextField" parent="NormalTextField">
	    <item name="android:textColor">@android:color/black</item>
		<item name="android:background">@drawable/search_text_field</item>
	</style>
	<style name="ContactSearchTextField" parent="NormalTextField">
	    <item name="android:textColor">@android:color/black</item>
	    <item name="android:textSize">@dimen/smaller_font</item>
	    <item name="android:paddingTop">5dp</item>
		<item name="android:paddingBottom">5dp</item>
		<item name="android:background">@drawable/contact_search_text_field</item>
	</style>
	<style name="SignInTextField" parent="NormalTextField">
	    <item name="android:textColor">@android:color/black</item>
	    <item name="android:paddingLeft">30dp</item>
		<item name="android:background">@drawable/sign_in_text_field</item>
	</style>
	<style name="RegistrationTextField" parent="NormalTextField">
	    <item name="android:textColor">@android:color/black</item>
	    <item name="android:paddingTop">10dp</item>
	    <item name="android:paddingBottom">10dp</item>
	    <item name="android:paddingRight">0dp</item>
	    <item name="android:background">@drawable/registration_text_field</item>
	</style>
	<style name="OrangeButton">
	    <item name="android:textColor">@android:color/white</item>
		<item name="android:background">@drawable/orange_button</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="RedButton">
	    <item name="android:textColor">@android:color/white</item>
		<item name="android:background">@drawable/red_button</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="GreenButton">
	    <item name="android:textColor">@android:color/white</item>
		<item name="android:background">@drawable/new_green_button</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="BlueButton">
	    <item name="android:textColor">@android:color/white</item>
		<item name="android:background">@drawable/new_blue_button</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="WhiteButton">
		<item name="android:background">@drawable/white_button</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="POITextField">
	    <item name="android:textColor">@android:color/black</item>
		<item name="android:background">@drawable/poi_text_field</item>
	</style>
	<style name="DualTextFieldFirst" parent="NormalTextField">
		<item name="android:background">@drawable/dual_text_field_first</item>
	</style>
	<style name="DualTextFieldSecond" parent="NormalTextField">
		<item name="android:background">@drawable/dual_text_field_second</item>
	</style>
	<style name="TimeTextField" parent="NormalTextField">
	    <item name="android:textSize">@dimen/large_font</item>
	    <item name="android:textStyle">bold</item>
	    <item name="android:paddingTop">0dp</item>
		<item name="android:paddingRight">0dp</item>
		<item name="android:paddingBottom">0dp</item>
		<item name="android:paddingLeft">0dp</item>
		<item name="android:includeFontPadding">true</item>
		<item name="android:singleLine">true</item>
		<item name="android:gravity">center</item>
	</style>
	<style name="WeekdayCheckBox">
	    <item name="android:textSize">@dimen/smaller_font</item>
	    <item name="android:textColor">@color/weekday_button_text</item>
	    <item name="android:paddingTop">17dp</item>
		<item name="android:paddingRight">0dp</item>
		<item name="android:paddingBottom">17dp</item>
		<item name="android:paddingLeft">0dp</item>
		<item name="android:includeFontPadding">false</item>
		<item name="android:gravity">center</item>
		<item name="android:background">@drawable/weekday_checkbox</item>
    </style>
	<style name="LargeTextButton">
	    <item name="android:textSize">@dimen/large_font</item>
	    <item name="android:textColor">@color/button_text</item>
	    <item name="android:textStyle">bold</item>
	    <item name="android:paddingTop">10dp</item>
		<item name="android:paddingRight">44dp</item>
		<item name="android:paddingLeft">15dp</item>
		<item name="android:paddingBottom">10dp</item>
		<item name="android:includeFontPadding">false</item>
		<item name="android:gravity">left</item>
	</style>
	<style name="NextActionButton" parent="LargeTextButton">
		<item name="android:background">@drawable/next_action_button</item>
	</style>
	<style name="AddButton" parent="LargeTextButton">
		<item name="android:background">@drawable/add_button</item>
	</style>
	<style name="CancelButton" parent="LargeTextButton">
		<item name="android:background">@drawable/cancel_button</item>
	</style>
	<style name="PrevActionButton" parent="LargeTextButton">
		<item name="android:background">@drawable/prev_action_button</item>
	</style>
	<style name="ShareButton" parent="LargeTextButton">
		<item name="android:background">@drawable/share_button</item>
	</style>
	<style name="ShareBlueButton" parent="LargeTextButton">
		<item name="android:background">@drawable/share_blue_button</item>
	</style>
	<style name="SmallerTextButton">
	    <item name="android:textSize">@dimen/smaller_font</item>
	    <item name="android:textColor">@color/button_text</item>
	    <item name="android:textStyle">bold</item>
	    <item name="android:paddingTop">6dp</item>
		<item name="android:paddingRight">44dp</item>
		<item name="android:paddingBottom">2dp</item>
		<item name="android:paddingLeft">15dp</item>
		<item name="android:gravity">left</item>
		<item name="android:includeFontPadding">false</item>
	</style>
	<style name="popUpDialogButton" parent="@style/Widget.Sherlock.Light.ActionButton"/>
	<style name="PopUpDialog" parent="@style/Theme.Sherlock.Light.Dialog">
	    <item name="android:windowBackground">@android:color/transparent</item>
	    <item name="android:colorBackgroundCacheHint">@null</item>
	    <item name="android:windowIsTranslucent">true</item>
	    <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
	    <item name="android:windowNoTitle">true</item>
	    <item name="android:windowContentOverlay">@null</item>
	    <!--<item name="android:windowTitleStyle">@style/popUpDialogTitle</item>-->
	</style>
	<style name="FullScreenDialog" parent="@android:style/Theme.Light">
		<item name="android:textColor">#fff</item>
		<item name="android:windowBackground">@android:color/transparent</item>
		<item name="android:windowFrame">@null</item>
		<item name="android:backgroundDimEnabled">true</item>
		<item name="android:windowIsTranslucent">true</item>
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowFullscreen">false</item>
	</style>
	<style name="popUpDialogTitle">
		<item name="android:textColor">#000</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textStyle">bold</item>
	</style>
	<style name="ProgressBar" parent="@android:style/Widget.ProgressBar.Inverse" />
	<style name="HoloProgressBar">
        <item name="android:indeterminateDrawable">@drawable/progress_large_holo</item>
        <item name="android:minWidth">76dip</item>
        <item name="android:maxWidth">76dip</item>
        <item name="android:minHeight">76dip</item>
        <item name="android:maxHeight">76dip</item>
    </style>
    
	<style name="loginBox">
        <item name="android:layout_width">match_parent</item>
    </style>
</resources>
